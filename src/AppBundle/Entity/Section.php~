<?php

namespace AppBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Section
 *
 * @ORM\Table(name="section")
 * @ORM\Entity(repositoryClass="AppBundle\Repository\SectionRepository")
 */
class Section
{
    /**
     * @var int
     *
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;

    /**
     * @var int
     *
     * @ORM\Column(name="id_spec", type="integer")
     */
    private $idSpec;
    
    /**
     * @ORM\ManyToOne(targetEntity="SpecSection", inversedBy="sections")
     * @ORM\JoinColumn(name="id_spec", referencedColumnName="id")
     */
    private $spec;

    /**
     * @var string
     *
     * @ORM\Column(name="name", type="string", length=255)
     */
    private $name;


    /**
     * Get id
     *
     * @return int
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set idSpec
     *
     * @param integer $idSpec
     *
     * @return Section
     */
    public function setIdSpec($idSpec)
    {
        $this->idSpec = $idSpec;

        return $this;
    }

    /**
     * Get idSpec
     *
     * @return int
     */
    public function getIdSpec()
    {
        return $this->idSpec;
    }

    /**
     * Set name
     *
     * @param string $name
     *
     * @return Section
     */
    public function setName($name)
    {
        $this->name = $name;

        return $this;
    }

    /**
     * Get name
     *
     * @return string
     */
    public function getName()
    {
        return $this->name;
    }
}


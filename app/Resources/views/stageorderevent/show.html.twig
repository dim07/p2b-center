{% extends 'PetkoparaCrudGeneratorBundle::base.html.twig' %}

{% block title %}
{{ parent() }} - stageOrderEvent
{% endblock %}

{% block body %}

<div id="top" class="row">
    <div class="page-header">
        <h2>View stageOrderEvent <span class="glyphicon glyphicon-eye-open" aria-hidden="true"></span> </h2>
    </div>
</div>

<div class="row">
    <div class="col-md-6">
        <p><strong>Id</strong></p>
        <p>
            {{ stageOrderEvent.id }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Idorder</strong></p>
        <p>
            {{ stageOrderEvent.idOrder }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Ideventtype</strong></p>
        <p>
            {{ stageOrderEvent.idEventType }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Eventdate</strong></p>
        <p>
            {% if stageOrderEvent.eventDate %}{{ stageOrderEvent.eventDate|date('Y-m-d H:i:s') }}{% endif %}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Info</strong></p>
        <p>
            {{ stageOrderEvent.info }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>File</strong></p>
        <p>
            {{ stageOrderEvent.File }}
        </p>

    </div></div>

<hr/>


<div class="form-group">
    <a class="btn btn-default" href="{{ path('stageorderevent') }}">
        <span class="glyphicon glyphicon-list" aria-hidden="true"></span>
        Back to list
    </a>
    <a class="btn btn-primary" href="{{ path('stageorderevent_edit', { 'id':  stageOrderEvent.id }) }}">
        <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
        Edit
    </a>
    <a class="btn btn-info" href="{{ path('stageorderevent_new') }}">
        <span class="glyphicon glyphicon-plus" aria-hidden="true"></span>
        New
    </a>
    <form action="{{ path('stageorderevent_delete', { 'id': stageOrderEvent.id }) }}" method="post" style="display: inline-block">
        <input type="hidden" name="_method" value="DELETE" />
        {{ form_widget(delete_form) }}
        <button class="btn btn-danger" type="submit" onclick="return confirm('Are you sure');">
            <span class="glyphicon glyphicon-remove" aria-hidden="true"></span>
            Delete
        </button>
    </form>
</div>


{% endblock %}


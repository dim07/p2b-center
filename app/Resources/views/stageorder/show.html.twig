{% extends 'base.html.twig' %}

{% block title %}
{{ parent() }} - stageOrder
{% endblock %}

{% block body %}

<div id="top" class="row">
    <div class="page-header">
        <h2>View stageOrder <span class="glyphicon glyphicon-eye-open" aria-hidden="true"></span> </h2>
    </div>
</div>

<div class="row">
    <div class="col-md-6">
        <p><strong>Id</strong></p>
        <p>
            {{ stageOrder.id }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Idsection</strong></p>
        <p>
            {{ stageOrder.idSection }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Idstage</strong></p>
        <p>
            {{ stageOrder.idStage }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Islegalentity</strong></p>
        <p>
            {% if stageOrder.isLegalEntity %}Yes{% else %}No{% endif %}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Afterorderid</strong></p>
        <p>
            {{ stageOrder.AfterOrderId }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Cost</strong></p>
        <p>
            {{ stageOrder.cost }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Enddate</strong></p>
        <p>
            {% if stageOrder.endDate %}{{ stageOrder.endDate|date('Y-m-d') }}{% endif %}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Startdate</strong></p>
        <p>
            {% if stageOrder.startDate %}{{ stageOrder.startDate|date('Y-m-d') }}{% endif %}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Factcost</strong></p>
        <p>
            {{ stageOrder.factCost }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Factenddate</strong></p>
        <p>
            {% if stageOrder.factEndDate %}{{ stageOrder.factEndDate|date('Y-m-d') }}{% endif %}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Info</strong></p>
        <p>
            {{ stageOrder.info }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Ispublic</strong></p>
        <p>
            {% if stageOrder.isPublic %}Yes{% else %}No{% endif %}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Contractorid</strong></p>
        <p>
            {{ stageOrder.ContractorId }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Userid</strong></p>
        <p>
            {{ stageOrder.UserId }}
        </p>

    </div></div>

<hr/>


<div class="form-group">
    <a class="btn btn-default" href="{{ path('stageorder') }}">
        <span class="glyphicon glyphicon-list" aria-hidden="true"></span>
        Back to list
    </a>
    <a class="btn btn-primary" href="{{ path('stageorder_edit', { 'id':  stageOrder.id }) }}">
        <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
        Edit
    </a>
    <a class="btn btn-info" href="{{ path('stageorder_new') }}">
        <span class="glyphicon glyphicon-plus" aria-hidden="true"></span>
        New
    </a>
    <form action="{{ path('stageorder_delete', { 'id': stageOrder.id }) }}" method="post" style="display: inline-block">
        <input type="hidden" name="_method" value="DELETE" />
        {{ form_widget(delete_form) }}
        <button class="btn btn-danger" type="submit" onclick="return confirm('Are you sure');">
            <span class="glyphicon glyphicon-remove" aria-hidden="true"></span>
            Delete
        </button>
    </form>
</div>


{% endblock %}


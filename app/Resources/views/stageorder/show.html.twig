{% extends 'base.html.twig' %}

{% block title %}
{{ parent() }} - stageOrder
{% endblock %}

{% block body %}

<div id="top" class="row">
    <div class="page-header">
        <h2>View stageOrder <span class="glyphicon glyphicon-eye-open" aria-hidden="true"></span> </h2>
    </div>
</div>

<div class="row">
    <div class="col-md-12">
    <div class="col-md-6">
        <p><strong>Id</strong></p>
        <p>
            {{ stageOrder.id }}
        </p>

    </div>


    <div class="col-md-6">
        <p><strong>Islegalentity</strong></p>
        <p>
            {% if stageOrder.isLegalEntity %}Yes{% else %}No{% endif %}
        </p>

    </div>

    <div class="col-md-6">
        <p><strong>Cost</strong></p>
        <p>
            {{ stageOrder.cost }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Enddate</strong></p>
        <p>
            {% if stageOrder.endDate %}{{ stageOrder.endDate|date('Y-m-d') }}{% endif %}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Startdate</strong></p>
        <p>
            {% if stageOrder.startDate %}{{ stageOrder.startDate|date('Y-m-d') }}{% endif %}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Factcost</strong></p>
        <p>
            {{ stageOrder.factCost }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Factenddate</strong></p>
        <p>
            {% if stageOrder.factEndDate %}{{ stageOrder.factEndDate|date('Y-m-d') }}{% endif %}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Info</strong></p>
        <p>
            {{ stageOrder.info }}
        </p>

    </div>
    </div>    
</div>

<hr/>


<div class="form-group">
    <a class="btn btn-default" href="{{ path('stageorder') }}">
        <span class="glyphicon glyphicon-list" aria-hidden="true"></span>
        Back to list
    </a>
    <a class="btn btn-primary" href="{{ path('stageorder_edit', { 'id':  stageOrder.id }) }}">
        <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
        Edit
    </a>

</div>


{% endblock %}

